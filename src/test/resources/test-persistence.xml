<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="pf" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.di.pf.domain.Organization</class>      
        <class>com.di.pf.domain.Users</class> 
        <class>com.di.pf.domain.UserRoles</class>
        <class>com.di.pf.domain.common.ApplicantType</class>  
        <class>com.di.pf.domain.common.BuildingMainClass</class>  
        <class>com.di.pf.domain.common.OrgType</class>
        <class>com.di.pf.domain.common.Roles</class>
        <class>com.di.pf.domain.common.Territory</class>
        <class>com.di.pf.domain.common.TerritoryType</class>
        
        <properties>
            <property name="eclipselink.logging.level" value="WARNING"/>
        </properties>
    </persistence-unit>
</persistence>
